@model IEnumerable<PrayerTimes.Models.TimesForPrayer>
@*@model PrayerTimes.Models.TimesForPrayer*@
@{
    ViewBag.Title = "TimesForPrayers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Prayer Times</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrayerTime)
        </th>
        <th>
            
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrayerTime)
            </td>
            <td>
               @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
        </tr>
    }



